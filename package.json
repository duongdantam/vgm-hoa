{
  "name": "fuyin",
  "version": "0.0.0-development",
  "license": "MIT",
  "author": "Fuyin Developer",
  "description": "Fuyin Gospel Media",
  "scripts": {
    "ng": "nx",
    "postinstall": "node patch.js && node ./decorate-angular-cli.js && ngcc --properties es2017 browser module main && electron-builder install-app-deps && patch-package",
    "patch-package": "patch-package",
    "nx": "nx",
    "start": "ng serve",
    "start:ionic.fy": "npm run sw.ionic.fy && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "g.ionic.page.fy": "ng g @ionic/angular-toolkit:page --project ionic-fy",
    "g.ionic.component": "nx generate component --platforms=ionic",
    "sw.ionic.fy": "npx webpack build --config apps/ionic-fy/webpack.sw-config.js",
    "build.ionic.fy": "npm run sw.ionic.fy && nx build ionic-fy",
    "build.ionic.fy.prod": "npm run sw.ionic.fy && nx build ionic-fy --configuration=production",
    "build.api.prod": "nx build api --configuration=production",
    "docker.api": "npm run build.api.prod && docker-compose -f docker-compose-api.yml build",
    "docker.ionic.fy": "npm run prepare.ionic.fy && docker-compose -f docker-compose-fy.yml build",
    "prepare.ionic.fy": "npm run clean && npm run clean.ionic.fy && npm run build.ionic.fy.prod",
    "prepare.ionic.fy.ios": "npm run prepare.ionic.fy && cd apps/ionic-fy && npx rimraf -- ios && npm run cap.add.ios && node ios-patch.js && npm run icon.ios && npx cap sync ios",
    "prepare.ionic.fy.android": "npm run prepare.ionic.fy && cd apps/ionic-fy && npx rimraf -- android && npm run cap.add.android && node android-patch.js && npm run icon.android && npx cap sync android",
    "sync.ionic.fy.ios": "cd apps/ionic-fy && npx cap sync ios",
    "sync.ionic.fy.android": "cd apps/ionic-fy && npx cap sync android",
    "sync.ionic.fy": "cd apps/ionic-fy && npm run cap.copy",
    "open.ionic.fy.ios": "cd apps/ionic-fy && npm run cap.ios",
    "open.ionic.fy.android": "cd apps/ionic-fy && npm run cap.android",
    "clean.ionic.fy": "cd apps/ionic-fy && npx rimraf -- hooks node_modules platforms www plugins package-lock.json && npm i && rimraf -- package-lock.json",
    "build.electron.fy": "npm run prepare.electron.fy && ng build electron-fy --configuration=production --base-href ./",
    "build.electron.fy.local": "npm run build.electron.fy && electron dist/apps/electron-fy",
    "build.electron.fy.linux": "npm run build.electron.fy && cd dist/apps/electron-fy && npx electron-builder build --linux -c.electronVersion 11.1.1",
    "build.electron.fy.windows": "npm run build.electron.fy && cd dist/apps/electron-fy && npx electron-builder build --windows -c.electronVersion 11.1.1",
    "build.electron.fy.mac": "npm run build.electron.fy && cd dist/apps/electron-fy && npx electron-builder build --mac -c.electronVersion 11.1.1",
    "prepare.electron.fy": "tsc -p apps/electron-fy/tsconfig.json",
    "serve.electron.fy.target": "nx serve electron-fy",
    "serve.electron.fy": "wait-on http-get://localhost:4200/ && electron apps/electron-fy/src --serve",
    "start.electron.fy": "npm run prepare.electron.fy && npm-run-all -p serve.electron.fy.target serve.electron.fy",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
