    "rxjs": "~6.6.3",
    "semantic-release": "^18.0.1",
    "semver-compare": "^1.0.0",
    "tslib": "^2.0.0",
    "video.js": "^7.11.8",
    "videojs-contrib-quality-levels": "^2.1.0",
    "videojs-hls-quality-selector": "^1.1.4",
    "zone.js": "^0.10.2"
